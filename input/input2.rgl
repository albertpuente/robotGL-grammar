ACTIONS
    func moveBackwards x :
        //call tomato 180
        moveForward x
    endfunc

    func tomato y :
        while true do rotate 1 wend
    endfunc
ENDACTIONS

BEGIN

    a = 6
    z = 3
    initRobot a, z, 45
    moveForward 3
    box 0, 0
    moveTo 1, 1
    stopRobot 2
    obstacle 7, 7
    rotate 90
    moveForward 3
    while z < a do z = z+1 call moveBackwards 1 wend
    moveTo 6,6
    call tomato 1
    
END
