ACTIONS
    func moveBackwards x :
        //call tomato 180
        moveForward x
    endfunc

    func tomato y :
        while true do rotate 1 wend
    endfunc
ENDACTIONS

BEGIN

    a = 6
    z = 3
    initRobot a, z, 45
    moveForward 3
    i = 0
    while i < 10 do 
        mark 1, i 
        i = i + 1
    wend
    box 0, 0
    moveTo 1, 1
    stopRobot 2
    obstacle 7, 7
    moveForward 3
    while z < a do z = z+1 call moveBackwards 1 wend
    moveTo 6,6
    call tomato 1
    
END
