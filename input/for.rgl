DEFINITIONS

    func inverse a :
        if false then
            return 0
        else a = 0
        endif
    endfunc    
    
    action moveBackwards x :
        moveForward get inverse (x)
        return x
    endaction
    
ENDDEFINITIONS

BEGIN

    initRobot 0,0,0
    face east
    call potato ()    
    moveForward 4
    return 8
    call moveBackwards (4)
END
