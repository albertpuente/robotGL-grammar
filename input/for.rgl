DEFINITIONS

    func inverse a :
        return 0-a
    endfunc    
    
    action moveBackwards x :
        rotate sin(cos(sqrt(180%35)))
        moveForward inverse (inverse (x))
    endaction
    
ENDDEFINITIONS

BEGIN

    initRobot 0,0,0
    face east
    moveForward 4
    moveBackwards (4)
END
